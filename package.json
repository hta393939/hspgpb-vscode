{
  "name": "gpb-vscode",
  "displayName": "gpb-vscode",
  "description": "gpb file lite looker for VS Code",
  "version": "0.0.5",
  "publisher": "hta39393",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hta393939/gpb-vscode/"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gpb-vscode.helloWorld",
        "title": "GPB: Hello World",
        "category": "Paw Draw"
      },
      {
        "command": "gpb-vscode.pawDraw.new",
        "title": "GPB: Create new gpb Document",
        "category": "Paw Draw"
      }
    ],
    "customEditors": [
      {
        "viewType": "gpb-vscode.pawDraw",
        "displayName": "GPB: Paw Draw",
        "selector": [
          {
            "filenamePattern": "*.gpb"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  }
}
